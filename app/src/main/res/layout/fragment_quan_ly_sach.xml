    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragment.QuanLySachFragment">

    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/lvQuanLySach"
        tools:listitem="@layout/item_sach"/>
